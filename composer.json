{
        "name": "atk14/range-field",
        "type": "library",
        "description": "RangeField is a form field for entering numeric range from and to in ATK14 applications",
        "homepage": "https://github.com/atk14/RangeField",
        "license": ["MIT"],
        "authors": [
                {
                        "name": "ATK14 Development Team",
                        "homepage": "https://github.com/orgs/atk14/people"
                }
        ],
        "require": {
                "php": ">=5.3.0"
        },
        "require-dev": {
                "atk14/tester": "*",
                "atk14/forms": "dev-master",
                "atk14/core": "dev-master"
        },
        "autoload": {
                "classmap": ["src/app/fields", "src/app/widgets"]
        }
}
